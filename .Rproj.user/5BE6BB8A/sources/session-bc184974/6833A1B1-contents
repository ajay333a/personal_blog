library(readr)
Thesisdata <- read_csv("F:/R Language/Thesisdata.csv")

ths_data <- Thesisdata

Conidium_d <- ths_data %>% 
  filter(Species == "Conidium verigatum")

sps_d <-ths_data %>% group_by(Species)
ggpairs(Conidium_d, columns = 3:5,
        aes(color = Treatment))

y <- ths_data$Species
function(x, y){
  x <- ggpairs(y, columns = 3:5, aes(color = Treatment))
  return(y)
}

?function()

